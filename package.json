{
  "name": "renamer",
  "productName": "Renamer",
  "version": "0.5.0",
  "description": "Renamer app",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "test": "./node_modules/.bin/mocha",
    "build-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --prune=true --out=builds",
    "build-win": "electron-packager . renamer --overwrite --asar --platform=win32 --prune=true --out=builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Renamer\"",
    "build-linux": "electron-packager . renamer --overwrite --asar --platform=linux --arch=x64 --prune=true --out=builds"
  },
  "author": "Evgen Gruzinov",
  "license": "MIT",
  "dependencies": {
    "electron": "^2.0.0-beta.7",
    "jquery": "^3.3.1",
    "npm": "^6.0.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "spectron": "^3.8.0"
  },
  "devDependencies": {
    "electron-packager": "^12.0.1",
    "mocha": "^5.1.1"
  }
}
